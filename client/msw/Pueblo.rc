        MENUITEM "Submit &feedback...",         ID_HELP_PUEBLO_BUG_REPORT
        MENUITEM SEPARATOR
        MENUITEM "About &Chaco...",             ID_HELP_ABOUT_CHACO
        MENUITEM "About U&E...",                ID_HELP_ABOUT_UE
        MENUITEM "More about Pueblo...",        ID_HELP_MORE_PUEBLO
    END
END


