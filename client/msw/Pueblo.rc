        MENUITEM SEPARATOR
        MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
        MENUITEM SEPARATOR
        MENUITEM "Trace window",                ID_VIEW_TRACE
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&Index",                      ID_HELP_INDEX
        MENUITEM "&Using Help",                 ID_HELP_USING
        MENUITEM SEPARATOR
        MENUITEM "&About Pueblo...",            ID_HELP_ABOUT_PUEBLO
        MENUITEM "&Register Pueblo...",         ID_HELP_REGISTER_PUEBLO
        MENUITEM SEPARATOR
